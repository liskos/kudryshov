\u1055\'cf\u1088\'f0\u1086\'ee\u1090\'f2\u1080\'e8\u1074\'e2 \u1074\'e2\u1072\'e0\u1089\'f1 \u1090\'f2\u1086\'ee\u1095\'f7\u1085\'ed\u1086\'ee \u1077\'e5\u1089\'f1\u1090\'f2\u1100\'fc \u1079\'e7\u1072\'e0\u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0,\~\u8211\'96 \u1089\'f1\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1083\'eb \u1086\'ee\u1085\'ed.\~\u8211\'96 \u1071\'df \u1085\'ed\u1072\'e0\u1096\'f8\u1077\'e5\u1083\'eb \u1091\'f3 \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee \u1076\'e4\u1088\'f0\u1072\'e0\u1075\'e3\u1091\'f3\u1085\'ed\u1089\'f1\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee \u1082\'ea\u1072\'e0\u1087\'ef\u1080\'e8\u1090\'f2\u1072\'e0\u1085\'ed\u1072\'e0 \u1080\'e8 \u1077\'e5\u1097\'f9\u1077\'e5 \u1086\'ee\u1076\'e4\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee \u1075\'e3\u1086\'ee\u1089\'f1\u1087\'ef\u1086\'ee\u1076\'e4\u1080\'e8\u1085\'ed\u1072\'e0, \u1082\'ea\u1086\'ee\u1090\'f2\u1086\'ee\u1088\'f0\u1086\'ee\u1075\'e3\u1086\'ee \u1092\'f4\u1072\'e0\u1084\'ec\u1080\'e8\u1083\'eb\u1080\'e8\u1080\'e8 \u1085\'ed\u1077\'e5 \u1087\'ef\u1086\'ee\u1084\'ec\u1085\'ed\u1102\'fe. \u1071\'df \u1085\'ed\u1072\'e0 \u1084\'ec\u1080\'e8\u1085\'ed\u1091\'f3\u1090\'f2\u1091\'f3 \u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1085\'ed\u1086\'ee\u1074\'e2\u1080\'e8\u1083\'eb\u1089\'f1\u1103\'ff \u1074\'e2 \u1087\'ef\u1077\'e5\u1088\'f0\u1077\'e5\u1076\'e4\u1085\'ed\u1077\'e5\u1081\'e9, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1 \u1089\'f1\u1085\'ed\u1103\'ff\u1090\'f2\u1100\'fc \u1075\'e3\u1072\'e0\u1083\'eb\u1086\'ee\u1096\'f8\u1080\'e8. \u1059\'d3 \u1085\'ed\u1080\'e8\u1093\'f5 \u1073\'e1\u1099\'fb\u1083\'eb \u1091\'f3\u1078\'e6\u1072\'e0\u1089\'f1\u1085\'ed\u1099\'fb\u1081\'e9 \u1096\'f8\u1091\'f3\u1084\'ec \u1080\'e8 \u1089\'f1\u1087\'ef\u1086\'ee\u1088\'f0\u8230\'85 \u171\'ab\u1053\'cd\u1080\'e8 \u1079\'e7\u1072\'e0 \u1095\'f7\u1090\'f2\u1086\'ee \u1085\'ed\u1077\'e5 \u1089\'f1\u1086\'ee\u1075\'e3\u1083\'eb\u1072\'e0\u1096\'f8\u1091\'f3\u1089\'f1\u1100\'fc!\~\u8211\'96 \u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1083\'eb \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1080\'e8\u1081\'e9,\~\u8211\'96 \u1086\'ee\u1085\'ed \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff \u1086\'ee\u1089\'f1\u1082\'ea\u1086\'ee\u1088\'f0\u1073\'e1\u1080\'e8\u1083\'eb \u1087\'ef\u1091\'f3\u1073\'e1\u1083\'eb\u1080\'e8\u1095\'f7\u1085\'ed\u1086\'ee; \u1090\'f2\u1086\'ee\u1075\'e3\u1076\'e4\u1072\'e0 \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee \u1089\'f1\u1086\'ee\u1074\'e2\u1089\'f1\u1077\'e5\u1084\'ec \u1076\'e4\u1088\'f0\u1091\'f3\u1075\'e3\u1086\'ee\u1077\'e5\u8230\'85\u187\'bb \u8211\'96 \u171\'ab\u1050\'ca\u1072\'e0\u1082\'ea\u1086\'ee\u1077\'e5 \u1090\'f2\u1077\'e5\u1073\'e1\u1077\'e5 \u1076\'e4\u1077\'e5\u1083\'eb\u1086\'ee?\~\u8211\'96 \u1086\'ee\u1090\'f2\u1074\'e2\u1077\'e5\u1095\'f7\u1072\'e0\u1083\'eb \u1082\'ea\u1072\'e0\u1087\'ef\u1080\'e8\u1090\'f2\u1072\'e0\u1085\'ed,\~\u8211\'96 \u1103\'ff \u1074\'e2\u1089\'f1\u1077\'e5 \u1073\'e1\u1077\'e5\u1088\'f0\u1091\'f3 \u1085\'ed\u1072\'e0 \u1089\'f1\u1077\'e5\u1073\'e1\u1103\'ff. \u1071\'df \u1073\'e1\u1099\'fb\u1083\'eb \u1089\'f1\u1077\'e5\u1082\'ea\u1091\'f3\u1085\'ed\u1076\'e4\u1072\'e0\u1085\'ed\u1090\'f2\u1086\'ee\u1084\'ec \u1085\'ed\u1072\'e0 \u1087\'ef\u1103\'ff\u1090\'f2\u1080\'e8 \u1076\'e4\u1091\'f3\u1101\'fd\u1083\'eb\u1103\'ff\u1093\'f5 \u1080\'e8 \u1091\'f3\u1078\'e6 \u1079\'e7\u1085\'ed\u1072\'e0\u1102\'fe, \u1082\'ea\u1072\'e0\u1082\'ea \u1101\'fd\u1090\'f2\u1086\'ee \u1091\'f3\u1089\'f1\u1090\'f2\u1088\'f0\u1086\'ee\u1080\'e8\u1090\'f2\u1100\'fc. \u1071\'df \u1074\'e2\u1089\'f1\u1077\'e5 \u1087\'ef\u1088\'f0\u1080\'e8\u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1083\'eb. \u1055\'cf\u1086\'ee\u1078\'e6\u1072\'e0\u1083\'eb\u1091\'f3\u1081\'e9\u1089\'f1\u1090\'f2\u1072\'e0, \u1090\'f2\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1084\'ec\u1085\'ed\u1077\'e5 \u1085\'ed\u1077\'e5 \u1084\'ec\u1077\'e5\u1096\'f8\u1072\'e0\u1081\'e9. \u1055\'cf\u1086\'ee\u1089\'f1\u1090\'f2\u1088\'f0\u1072\'e0\u1097\'f9\u1072\'e0\u1090\'f2\u1100\'fc \u1085\'ed\u1077\'e5 \u1093\'f5\u1091\'f3\u1076\'e4\u1086\'ee. \u1040\'c0 \u1079\'e7\u1072\'e0\u1095\'f7\u1077\'e5\u1084\'ec \u1087\'ef\u1086\'ee\u1076\'e4\u1074\'e2\u1077\'e5\u1088\'f0\u1075\'e3\u1072\'e0\u1090\'f2\u1100\'fc \u1089\'f1\u1077\'e5\u1073\'e1\u1103\'ff \u1086\'ee\u1087\'ef\u1072\'e0\u1089\'f1\u1085\'ed\u1086\'ee\u1089\'f1\u1090\'f2\u1080\'e8, \u1077\'e5\u1089\'f1\u1083\'eb\u1080\'e8 \u1084\'ec\u1086\'ee\u1078\'e6\u1085\'ed\u1086\'ee \u1080\'e8\u1079\'e7\u1073\'e1\u1072\'e0\u1074\'e2\u1080\'e8\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff?..\u187\'bb \u1042\'c2 \u1101\'fd\u1090\'f2\u1091\'f3 \u1084\'ec\u1080\'e8\u1085\'ed\u1091\'f3\u1090\'f2\u1091\'f3 \u1103\'ff \u1074\'e2\u1079\'e7\u1086\'ee\u1096\'f8\u1077\'e5\u1083\'eb. \u1054\'ce\u1085\'ed\u1080\'e8 \u1079\'e7\u1072\'e0\u1084\'ec\u1086\'ee\u1083\'eb\u1095\'f7\u1072\'e0\u1083\'eb\u1080\'e8. \u1055\'cf\u1077\'e5\u1088\'f0\u1077\'e5\u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0\u1099\'fb \u1085\'ed\u1072\'e0\u1096\'f8\u1080\'e8 \u1087\'ef\u1088\'f0\u1086\'ee\u1076\'e4\u1086\'ee\u1083\'eb\u1078\'e6\u1072\'e0\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc \u1076\'e4\u1086\'ee\u1074\'e2\u1086\'ee\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1076\'e4\u1086\'ee\u1083\'eb\u1075\'e3\u1086\'ee; \u1085\'ed\u1072\'e0\u1082\'ea\u1086\'ee\u1085\'ed\u1077\'e5\u1094\'f6 \u1084\'ec\u1099\'fb \u1088\'f0\u1077\'e5\u1096\'f8\u1080\'e8\u1083\'eb\u1080\'e8 \u1076\'e4\u1077\'e5\u1083\'eb\u1086\'ee \u1074\'e2\u1086\'ee\u1090\'f2 \u1082\'ea\u1072\'e0\u1082\'ea: \u1074\'e2\u1077\'e5\u1088\'f0\u1089\'f1\u1090\'f2\u1072\'e0\u1093\'f5 \u1074\'e2 \u1087\'ef\u1103\'ff\u1090\'f2\u1080\'e8 \u1086\'ee\u1090\'f2\u1089\'f1\u1102\'fe\u1076\'e4\u1072\'e0 \u1077\'e5\u1089\'f1\u1090\'f2\u1100\'fc \u1075\'e3\u1083\'eb\u1091\'f3\u1093\'f5\u1086\'ee\u1077\'e5 \u1091\'f3\u1097\'f9\u1077\'e5\u1083\'eb\u1100\'fc\u1077\'e5; \u1086\'ee\u1085\'ed\u1080\'e8 \u1090\'f2\u1091\'f3\u1076\'e4\u1072\'e0 \u1087\'ef\u1086\'ee\u1077\'e5\u1076\'e4\u1091\'f3\u1090\'f2 \u1079\'e7\u1072\'e0\u1074\'e2\u1090\'f2\u1088\'f0\u1072\'e0 \u1074\'e2 \u1095\'f7\u1077\'e5\u1090\'f2\u1099\'fb\u1088\'f0\u1077\'e5 \u1095\'f7\u1072\'e0\u1089\'f1\u1072\'e0 \u1091\'f3\u1090\'f2\u1088\'f0\u1072\'e0, \u1072\'e0 \u1084\'ec\u1099\'fb \u1074\'e2\u1099\'fb\u1077\'e5\u1076\'e4\u1077\'e5\u1084\'ec \u1087\'ef\u1086\'ee\u1083\'eb\u1095\'f7\u1072\'e0\u1089\'f1\u1072\'e0 \u1087\'ef\u1086\'ee\u1089\'f1\u1083\'eb\u1077\'e5 \u1085\'ed\u1080\'e8\u1093\'f5; \u1089\'f1\u1090\'f2\u1088\'f0\u1077\'e5\u1083\'eb\u1103\'ff\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1073\'e1\u1091\'f3\u1076\'e4\u1077\'e5\u1090\'f2\u1077\'e5 \u1085\'ed\u1072\'e0 \u1096\'f8\u1077\'e5\u1089\'f1\u1090\'f2\u1080\'e8 \u1096\'f8\u1072\'e0\u1075\'e3\u1072\'e0\u1093\'f5 \u8211\'96 \u1101\'fd\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u1090\'f2\u1088\'f0\u1077\'e5\u1073\'e1\u1086\'ee\u1074\'e2\u1072\'e0\u1083\'eb \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1080\'e8\u1081\'e9. \u1059\'d3\u1073\'e1\u1080\'e8\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u8211\'96 \u1085\'ed\u1072\'e0 \u1089\'f1\u1095\'f7\u1077\'e5\u1090\'f2 \u1095\'f7\u1077\'e5\u1088\'f0\u1082\'ea\u1077\'e5\u1089\'f1\u1086\'ee\u1074\'e2. \u1058\'d2\u1077\'e5\u1087\'ef\u1077\'e5\u1088\'f0\u1100\'fc \u1074\'e2\u1086\'ee\u1090\'f2 \u1082\'ea\u1072\'e0\u1082\'ea\u1080\'e8\u1077\'e5 \u1091\'f3 \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff \u1087\'ef\u1086\'ee\u1076\'e4\u1086\'ee\u1079\'e7\u1088\'f0\u1077\'e5\u1085\'ed\u1080\'e8\u1103\'ff: \u1086\'ee\u1085\'ed\u1080\'e8, \u1090\'f2\u1086\'ee \u1077\'e5\u1089\'f1\u1090\'f2\u1100\'fc \u1089\'f1\u1077\'e5\u1082\'ea\u1091\'f3\u1085\'ed\u1076\'e4\u1072\'e0\u1085\'ed\u1090\'f2\u1099\'fb, \u1076\'e4\u1086\'ee\u1083\'eb\u1078\'e6\u1085\'ed\u1086\'ee \u1073\'e1\u1099\'fb\u1090\'f2\u1100\'fc, \u1085\'ed\u1077\'e5\u1089\'f1\u1082\'ea\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1087\'ef\u1077\'e5\u1088\'f0\u1077\'e5\u1084\'ec\u1077\'e5\u1085\'ed\u1080\'e8\u1083\'eb\u1080\'e8 \u1089\'f1\u1074\'e2\u1086\'ee\u1081\'e9 \u1087\'ef\u1088\'f0\u1077\'e5\u1078\'e6\u1085\'ed\u1080\'e8\u1081\'e9 \u1087\'ef\u1083\'eb\u1072\'e0\u1085\'ed \u1080\'e8 \u1093\'f5\u1086\'ee\u1090\'f2\u1103\'ff\u1090\'f2 \u1079\'e7\u1072\'e0\u1088\'f0\u1103\'ff\u1076\'e4\u1080\'e8\u1090\'f2\u1100\'fc \u1087\'ef\u1091\'f3\u1083\'eb\u1077\'e5\u1102\'fe \u1086\'ee\u1076\'e4\u1080\'e8\u1085\'ed \u1087\'ef\u1080\'e8\u1089\'f1\u1090\'f2\u1086\'ee\u1083\'eb\u1077\'e5\u1090\'f2 \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee. \u1069\'dd\u1090\'f2\u1086\'ee \u1085\'ed\u1077\'e5\u1084\'ec\u1085\'ed\u1086\'ee\u1078\'e6\u1082\'ea\u1086\'ee \u1087\'ef\u1086\'ee\u1093\'f5\u1086\'ee\u1078\'e6\u1077\'e5 \u1085\'ed\u1072\'e0 \u1091\'f3\u1073\'e1\u1080\'e8\u1081\'e9\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee, \u1085\'ed\u1086\'ee \u1074\'e2 \u1074\'e2\u1086\'ee\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1077\'e5 \u1074\'e2\u1088\'f0\u1077\'e5\u1084\'ec\u1103\'ff, \u1080\'e8 \u1086\'ee\u1089\'f1\u1086\'ee\u1073\'e1\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee \u1074\'e2 \u1072\'e0\u1079\'e7\u1080\'e8\u1072\'e0\u1090\'f2\u1089\'f1\u1082\'ea\u1086\'ee\u1081\'e9 \u1074\'e2\u1086\'ee\u1081\'e9\u1085\'ed\u1077\'e5, \u1093\'f5\u1080\'e8\u1090\'f2\u1088\'f0\u1086\'ee\u1089\'f1\u1090\'f2\u1080\'e8 \u1087\'ef\u1086\'ee\u1079\'e7\u1074\'e2\u1086\'ee\u1083\'eb\u1103\'ff\u1102\'fe\u1090\'f2\u1089\'f1\u1103\'ff; \u1090\'f2\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1080\'e8\u1081\'e9, \u1082\'ea\u1072\'e0\u1078\'e6\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff, \u1087\'ef\u1086\'ee\u1073\'e1\u1083\'eb\u1072\'e0\u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4\u1085\'ed\u1077\'e5\u1077\'e5 \u1089\'f1\u1074\'e2\u1086\'ee\u1080\'e8\u1093\'f5 \u1090\'f2\u1086\'ee\u1074\'e2\u1072\'e0\u1088\'f0\u1080\'e8\u1097\'f9\u1077\'e5\u1081\'e9. \u1050\'ca\u1072\'e0\u1082\'ea \u1074\'e2\u1099\'fb \u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1077\'e5\u1090\'f2\u1077\'e5? \u1044\'c4\u1086\'ee\u1083\'eb\u1078\'e6\u1085\'ed\u1099\'fb \u1083\'eb\u1080\'e8 \u1084\'ec\u1099\'fb \u1087\'ef\u1086\'ee\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1090\'f2\u1100\'fc \u1080\'e8\u1084\'ec, \u1095\'f7\u1090\'f2\u1086\'ee \u1076\'e4\u1086\'ee\u1075\'e3\u1072\'e0\u1076\'e4\u1072\'e0\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc?}
\par \pard\plain \s28\rtlch\afs24\af5\alang1025\ai0\ab0 \ltrch\lang1049\langfe1049\hich\af5\loch\qj\nowidctlpar\hyphpar0\li0\ri0\lin0\rin0\fi567\sl240\slmult1\ltrpar\f5\fs24\scaps0\caps0\cf0\outl0\strike0\expnd0\expndtw0\lang1049\i0\ulnone\ulc0\b0\kerning0\accnone\charscalex100\dbch\af5\langfe1049{
