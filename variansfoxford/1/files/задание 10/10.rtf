\u1055\'cf\u1086\'ee\u1078\'e6\u1072\'e0\u1083\'eb\u1091\'f3\u1081\'e9!\~\u8211\'96 \u1089\'f1\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1083\'eb \u1076\'e4\u1088\'f0\u1072\'e0\u1075\'e3\u1091\'f3\u1085\'ed\u1089\'f1\u1082\'ea\u1080\'e8\u1081\'e9 \u1082\'ea\u1072\'e0\u1087\'ef\u1080\'e8\u1090\'f2\u1072\'e0\u1085\'ed, \u1087\'ef\u1086\'ee\u1089\'f1\u1084\'ec\u1086\'ee\u1090\'f2\u1088\'f0\u1077\'e5\u1074\'e2 \u1074\'e2\u1099\'fb\u1088\'f0\u1072\'e0\u1079\'e7\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1085\'ed\u1072\'e0 \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee, \u1082\'ea\u1086\'ee\u1090\'f2\u1086\'ee\u1088\'f0\u1099\'fb\u1081\'e9 \u1082\'ea\u1080\'e8\u1074\'e2\u1085\'ed\u1091\'f3\u1083\'eb \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1074\'e2\u1086\'ee\u1081\'e9 \u1074\'e2 \u1079\'e7\u1085\'ed\u1072\'e0\u1082\'ea \u1089\'f1\u1086\'ee\u1075\'e3\u1083\'eb\u1072\'e0\u1089\'f1\u1080\'e8\u1103\'ff. \u1051\'cb\u1080\'e8\u1094\'f6\u1086\'ee \u1077\'e5\u1075\'e3\u1086\'ee \u1077\'e5\u1078\'e6\u1077\'e5\u1084\'ec\u1080\'e8\u1085\'ed\u1091\'f3\u1090\'f2\u1085\'ed\u1086\'ee \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff\u1083\'eb\u1086\'ee\u1089\'f1\u1100\'fc. \u1071\'df \u1077\'e5\u1075\'e3\u1086\'ee \u1087\'ef\u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1074\'e2\u1080\'e8\u1083\'eb \u1074\'e2 \u1079\'e7\u1072\'e0\u1090\'f2\u1088\'f0\u1091\'f3\u1076\'e4\u1085\'ed\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1077\'e5 \u1087\'ef\u1086\'ee\u1083\'eb\u1086\'ee\u1078\'e6\u1077\'e5\u1085\'ed\u1080\'e8\u1077\'e5. \u1057\'d1\u1090\'f2\u1088\'f0\u1077\'e5\u1083\'eb\u1103\'ff\u1103\'ff\u1089\'f1\u1100\'fc \u1087\'ef\u1088\'f0\u1080\'e8 \u1086\'ee\u1073\'e1\u1099\'fb\u1082\'ea\u1085\'ed\u1086\'ee\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1099\'fb\u1093\'f5 \u1091\'f3\u1089\'f1\u1083\'eb\u1086\'ee\u1074\'e2\u1080\'e8\u1103\'ff\u1093\'f5, \u1086\'ee\u1085\'ed \u1084\'ec\u1086\'ee\u1075\'e3 \u1094\'f6\u1077\'e5\u1083\'eb\u1080\'e8\u1090\'f2\u1100\'fc \u1084\'ec\u1085\'ed\u1077\'e5 \u1074\'e2 \u1085\'ed\u1086\'ee\u1075\'e3\u1091\'f3, \u1083\'eb\u1077\'e5\u1075\'e3\u1082\'ea\u1086\'ee \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff \u1088\'f0\u1072\'e0\u1085\'ed\u1080\'e8\u1090\'f2\u1100\'fc \u1080\'e8 \u1091\'f3\u1076\'e4\u1086\'ee\u1074\'e2\u1083\'eb\u1077\'e5\u1090\'f2\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1090\'f2\u1100\'fc \u1090\'f2\u1072\'e0\u1082\'ea\u1080\'e8\u1084\'ec \u1086\'ee\u1073\'e1\u1088\'f0\u1072\'e0\u1079\'e7\u1086\'ee\u1084\'ec \u1089\'f1\u1074\'e2\u1086\'ee\u1102\'fe \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1100\'fc, \u1085\'ed\u1077\'e5 \u1086\'ee\u1090\'f2\u1103\'ff\u1075\'e3\u1086\'ee\u1097\'f9\u1072\'e0\u1103\'ff \u1089\'f1\u1083\'eb\u1080\'e8\u1096\'f8\u1082\'ea\u1086\'ee\u1084\'ec \u1089\'f1\u1074\'e2\u1086\'ee\u1077\'e5\u1081\'e9 \u1089\'f1\u1086\'ee\u1074\'e2\u1077\'e5\u1089\'f1\u1090\'f2\u1080\'e8; \u1085\'ed\u1086\'ee \u1090\'f2\u1077\'e5\u1087\'ef\u1077\'e5\u1088\'f0\u1100\'fc \u1086\'ee\u1085\'ed \u1076\'e4\u1086\'ee\u1083\'eb\u1078\'e6\u1077\'e5\u1085\'ed \u1073\'e1\u1099\'fb\u1083\'eb \u1074\'e2\u1099\'fb\u1089\'f1\u1090\'f2\u1088\'f0\u1077\'e5\u1083\'eb\u1080\'e8\u1090\'f2\u1100\'fc \u1085\'ed\u1072\'e0 \u1074\'e2\u1086\'ee\u1079\'e7\u1076\'e4\u1091\'f3\u1093\'f5, \u1080\'e8\u1083\'eb\u1080\'e8 \u1089\'f1\u1076\'e4\u1077\'e5\u1083\'eb\u1072\'e0\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1091\'f3\u1073\'e1\u1080\'e8\u1081\'e9\u1094\'f6\u1077\'e5\u1081\'e9, \u1080\'e8\u1083\'eb\u1080\'e8, \u1085\'ed\u1072\'e0\u1082\'ea\u1086\'ee\u1085\'ed\u1077\'e5\u1094\'f6, \u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1074\'e2\u1080\'e8\u1090\'f2\u1100\'fc \u1089\'f1\u1074\'e2\u1086\'ee\u1081\'e9 \u1087\'ef\u1086\'ee\u1076\'e4\u1083\'eb\u1099\'fb\u1081\'e9 \u1079\'e7\u1072\'e0\u1084\'ec\u1099\'fb\u1089\'f1\u1077\'e5\u1083\'eb \u1080\'e8 \u1087\'ef\u1086\'ee\u1076\'e4\u1074\'e2\u1077\'e5\u1088\'f0\u1075\'e3\u1085\'ed\u1091\'f3\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1086\'ee\u1076\'e4\u1080\'e8\u1085\'ed\u1072\'e0\u1082\'ea\u1086\'ee\u1074\'e2\u1086\'ee\u1081\'e9 \u1089\'f1\u1086\'ee \u1084\'ec\u1085\'ed\u1086\'ee\u1102\'fe \u1086\'ee\u1087\'ef\u1072\'e0\u1089\'f1\u1085\'ed\u1086\'ee\u1089\'f1\u1090\'f2\u1080\'e8. \u1042\'c2 \u1101\'fd\u1090\'f2\u1091\'f3 \u1084\'ec\u1080\'e8\u1085\'ed\u1091\'f3\u1090\'f2\u1091\'f3 \u1103\'ff \u1085\'ed\u1077\'e5 \u1078\'e6\u1077\'e5\u1083\'eb\u1072\'e0\u1083\'eb \u1073\'e1\u1099\'fb \u1073\'e1\u1099\'fb\u1090\'f2\u1100\'fc \u1085\'ed\u1072\'e0 \u1077\'e5\u1075\'e3\u1086\'ee \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1077\'e5. \u1054\'ce\u1085\'ed \u1086\'ee\u1090\'f2\u1074\'e2\u1077\'e5\u1083\'eb \u1082\'ea\u1072\'e0\u1087\'ef\u1080\'e8\u1090\'f2\u1072\'e0\u1085\'ed\u1072\'e0 \u1074\'e2 \u1089\'f1\u1090\'f2\u1086\'ee\u1088\'f0\u1086\'ee\u1085\'ed\u1091\'f3 \u1080\'e8 \u1089\'f1\u1090\'f2\u1072\'e0\u1083\'eb \u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1090\'f2\u1100\'fc \u1077\'e5\u1084\'ec\u1091\'f3 \u1095\'f7\u1090\'f2\u1086\'ee-\u1090\'f2\u1086\'ee \u1089\'f1 \u1073\'e1\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8\u1080\'e8\u1084\'ec \u1078\'e6\u1072\'e0\u1088\'f0\u1086\'ee\u1084\'ec; \u1103\'ff \u1074\'e2\u1080\'e8\u1076\'e4\u1077\'e5\u1083\'eb, \u1082\'ea\u1072\'e0\u1082\'ea \u1087\'ef\u1086\'ee\u1089\'f1\u1080\'e8\u1085\'ed\u1077\'e5\u1074\'e2\u1096\'f8\u1080\'e8\u1077\'e5 \u1075\'e3\u1091\'f3\u1073\'e1\u1099\'fb \u1077\'e5\u1075\'e3\u1086\'ee \u1076\'e4\u1088\'f0\u1086\'ee\u1078\'e6\u1072\'e0\u1083\'eb\u1080\'e8; \u1085\'ed\u1086\'ee \u1082\'ea\u1072\'e0\u1087\'ef\u1080\'e8\u1090\'f2\u1072\'e0\u1085\'ed \u1086\'ee\u1090\'f2 \u1085\'ed\u1077\'e5\u1075\'e3\u1086\'ee \u1086\'ee\u1090\'f2\u1074\'e2\u1077\'e5\u1088\'f0\u1085\'ed\u1091\'f3\u1083\'eb\u1089\'f1\u1103\'ff \u1089\'f1 \u1087\'ef\u1088\'f0\u1077\'e5\u1079\'e7\u1088\'f0\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1081\'e9 \u1091\'f3\u1083\'eb\u1099\'fb\u1073\'e1\u1082\'ea\u1086\'ee\u1081\'e9. \u171\'ab\u1058\'d2\u1099\'fb \u1076\'e4\u1091\'f3\u1088\'f0\u1072\'e0\u1082\'ea!\~\u8211\'96 \u1089\'f1\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1083\'eb \u1086\'ee\u1085\'ed \u1043\'c3\u1088\'f0\u1091\'f3\u1096\'f8\u1085\'ed\u1080\'e8\u1094\'f6\u1082\'ea\u1086\'ee\u1084\'ec\u1091\'f3 \u1076\'e4\u1086\'ee\u1074\'e2\u1086\'ee\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1075\'e3\u1088\'f0\u1086\'ee\u1084\'ec\u1082\'ea\u1086\'ee,\~\u8211\'96 \u1085\'ed\u1080\'e8\u1095\'f7\u1077\'e5\u1075\'e3\u1086\'ee \u1085\'ed\u1077\'e5 \u1087\'ef\u1086\'ee\u1085\'ed\u1080\'e8\u1084\'ec\u1072\'e0\u1077\'e5\u1096\'f8\u1100\'fc! \u1054\'ce\u1090\'f2\u1087\'ef\u1088\'f0\u1072\'e0\u1074\'e2\u1080\'e8\u1084\'ec\u1090\'f2\u1077\'e5\u1089\'f1\u1100\'fc \u1078\'e6\u1077\'e5, \u1075\'e3\u1086\'ee\u1089\'f1\u1087\'ef\u1086\'ee\u1076\'e4\u1072\'e0!\u187\'bb}
