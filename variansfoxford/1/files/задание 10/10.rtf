\u1042\'c2\u1099\'fb \u1089\'f1\u1082\'ea\u1072\'e0\u1078\'e6\u1077\'e5\u1090\'f2\u1077\'e5, \u1095\'f7\u1090\'f2\u1086\'ee \u1085\'ed\u1088\'f0\u1072\'e0\u1074\'e2\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1089\'f1\u1090\'f2\u1100\'fc \u1086\'ee\u1090\'f2 \u1101\'fd\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u1085\'ed\u1077\'e5 \u1074\'e2\u1099\'fb\u1080\'e8\u1075\'e3\u1088\'f0\u1099\'fb\u1074\'e2\u1072\'e0\u1077\'e5\u1090\'f2? \u1048\'c8\u1079\'e7\u1074\'e2\u1080\'e8\u1085\'ed\u1080\'e8\u1090\'f2\u1077\'e5. \u1044\'c4\u1086\'ee\u1074\'e2\u1086\'ee\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1083\'eb\u1102\'fe\u1076\'e4\u1077\'e5\u1081\'e9 \u1082\'ea\u1086\'ee\u1088\'f0\u1084\'ec\u1080\'e8\u1083\'eb\u1080\'e8 \u1089\'f1\u1083\'eb\u1072\'e0\u1089\'f1\u1090\'f2\u1103\'ff\u1084\'ec\u1080\'e8; \u1091\'f3 \u1085\'ed\u1080\'e8\u1093\'f5 \u1086\'ee\u1090\'f2 \u1101\'fd\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u1080\'e8\u1089\'f1\u1087\'ef\u1086\'ee\u1088\'f0\u1090\'f2\u1080\'e8\u1083\'eb\u1089\'f1\u1103\'ff \u1078\'e6\u1077\'e5\u1083\'eb\u1091\'f3\u1076\'e4\u1086\'ee\u1082\'ea: \u1085\'ed\u1091\'f3\u1078\'e6\u1085\'ed\u1099\'fb \u1075\'e3\u1086\'ee\u1088\'f0\u1100\'fc\u1082\'ea\u1080\'e8\u1077\'e5 \u1083\'eb\u1077\'e5\u1082\'ea\u1072\'e0\u1088\'f0\u1089\'f1\u1090\'f2\u1074\'e2\u1072\'e0, \u1077\'e5\u1076\'e4\u1082\'ea\u1080\'e8\u1077\'e5 \u1080\'e8\u1089\'f1\u1090\'f2\u1080\'e8\u1085\'ed\u1099\'fb. \u1053\'cd\u1086\'ee \u1085\'ed\u1077\'e5 \u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1081\'e9\u1090\'f2\u1077\'e5, \u1086\'ee\u1076\'e4\u1085\'ed\u1072\'e0\u1082\'ea\u1086\'ee, \u1087\'ef\u1086\'ee\u1089\'f1\u1083\'eb\u1077\'e5 \u1101\'fd\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1 \u1072\'e0\u1074\'e2\u1090\'f2\u1086\'ee\u1088\'f0 \u1101\'fd\u1090\'f2\u1086\'ee\u1081\'e9 \u1082\'ea\u1085\'ed\u1080\'e8\u1075\'e3\u1080\'e8 \u1080\'e8\u1084\'ec\u1077\'e5\u1083\'eb \u1082\'ea\u1086\'ee\u1075\'e3\u1076\'e4\u1072\'e0-\u1085\'ed\u1080\'e8\u1073\'e1\u1091\'f3\u1076\'e4\u1100\'fc \u1075\'e3\u1086\'ee\u1088\'f0\u1076\'e4\u1091\'f3\u1102\'fe \u1084\'ec\u1077\'e5\u1095\'f7\u1090\'f2\u1091\'f3 \u1089\'f1\u1076\'e4\u1077\'e5\u1083\'eb\u1072\'e0\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1080\'e8\u1089\'f1\u1087\'ef\u1088\'f0\u1072\'e0\u1074\'e2\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1077\'e5\u1084\'ec \u1083\'eb\u1102\'fe\u1076\'e4\u1089\'f1\u1082\'ea\u1080\'e8\u1093\'f5 \u1087\'ef\u1086\'ee\u1088\'f0\u1086\'ee\u1082\'ea\u1086\'ee\u1074\'e2. \u1041\'c1\u1086\'ee\u1078\'e6\u1077\'e5 \u1077\'e5\u1075\'e3\u1086\'ee \u1080\'e8\u1079\'e7\u1073\'e1\u1072\'e0\u1074\'e2\u1080\'e8 \u1086\'ee\u1090\'f2 \u1090\'f2\u1072\'e0\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee \u1085\'ed\u1077\'e5\u1074\'e2\u1077\'e5\u1078\'e6\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1072\'e0! \u1045\'c5\u1084\'ec\u1091\'f3 \u1087\'ef\u1088\'f0\u1086\'ee\u1089\'f1\u1090\'f2\u1086\'ee \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee \u1074\'e2\u1077\'e5\u1089\'f1\u1077\'e5\u1083\'eb\u1086\'ee \u1088\'f0\u1080\'e8\u1089\'f1\u1086\'ee\u1074\'e2\u1072\'e0\u1090\'f2\u1100\'fc \u1089\'f1\u1086\'ee\u1074\'e2\u1088\'f0\u1077\'e5\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee \u1095\'f7\u1077\'e5\u1083\'eb\u1086\'ee\u1074\'e2\u1077\'e5\u1082\'ea\u1072\'e0, \u1082\'ea\u1072\'e0\u1082\'ea\u1080\'e8\u1084\'ec \u1086\'ee\u1085\'ed \u1077\'e5\u1075\'e3\u1086\'ee \u1087\'ef\u1086\'ee\u1085\'ed\u1080\'e8\u1084\'ec\u1072\'e0\u1077\'e5\u1090\'f2, \u1080\'e8 \u1082\'ea \u1077\'e5\u1075\'e3\u1086\'ee \u1080\'e8 \u1074\'e2\u1072\'e0\u1096\'f8\u1077\'e5\u1084\'ec\u1091\'f3 \u1085\'ed\u1077\'e5\u1089\'f1\u1095\'f7\u1072\'e0\u1089\'f1\u1090\'f2\u1100\'fc\u1102\'fe, \u1089\'f1\u1083\'eb\u1080\'e8\u1096\'f8\u1082\'ea\u1086\'ee\u1084\'ec \u1095\'f7\u1072\'e0\u1089\'f1\u1090\'f2\u1086\'ee \u1074\'e2\u1089\'f1\u1090\'f2\u1088\'f0\u1077\'e5\u1095\'f7\u1072\'e0\u1083\'eb. \u1041\'c1\u1091\'f3\u1076\'e4\u1077\'e5\u1090\'f2 \u1080\'e8 \u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee \u1073\'e1\u1086\'ee\u1083\'eb\u1077\'e5\u1079\'e7\u1085\'ed\u1100\'fc \u1091\'f3\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1085\'ed\u1072\'e0, \u1072\'e0 \u1082\'ea\u1072\'e0\u1082\'ea \u1077\'e5\u1077\'e5 \u1080\'e8\u1079\'e7\u1083\'eb\u1077\'e5\u1095\'f7\u1080\'e8\u1090\'f2\u1100\'fc \u8211\'96 \u1101\'fd\u1090\'f2\u1086\'ee \u1091\'f3\u1078\'e6 \u1073\'e1\u1086\'ee\u1075\'e3 \u1079\'e7\u1085\'ed\u1072\'e0\u1077\'e5\u1090\'f2!}
