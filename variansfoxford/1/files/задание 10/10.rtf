\u1057\'d1\u1077\'e5\u1075\'e3\u1086\'ee\u1076\'e4\u1085\'ed\u1103\'ff \u1103\'ff \u1074\'e2\u1089\'f1\u1090\'f2\u1072\'e0\u1083\'eb \u1087\'ef\u1086\'ee\u1079\'e7\u1076\'e4\u1085\'ed\u1086\'ee; \u1087\'ef\u1088\'f0\u1080\'e8\u1093\'f5\u1086\'ee\u1078\'e6\u1091\'f3 \u1082\'ea \u1082\'ea\u1086\'ee\u1083\'eb\u1086\'ee\u1076\'e4\u1094\'f6\u1091\'f3 \u8211\'96 \u1085\'ed\u1080\'e8\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee \u1091\'f3\u1078\'e6\u1077\'e5 \u1085\'ed\u1077\'e5\u1090\'f2. \u1057\'d1\u1090\'f2\u1072\'e0\u1085\'ed\u1086\'ee\u1074\'e2\u1080\'e8\u1083\'eb\u1086\'ee\u1089\'f1\u1100\'fc \u1078\'e6\u1072\'e0\u1088\'f0\u1082\'ea\u1086\'ee; \u1073\'e1\u1077\'e5\u1083\'eb\u1099\'fb\u1077\'e5 \u1084\'ec\u1086\'ee\u1093\'f5\u1085\'ed\u1072\'e0\u1090\'f2\u1099\'fb\u1077\'e5 \u1090\'f2\u1091\'f3\u1095\'f7\u1082\'ea\u1080\'e8 \u1073\'e1\u1099\'fb\u1089\'f1\u1090\'f2\u1088\'f0\u1086\'ee \u1073\'e1\u1077\'e5\u1078\'e6\u1072\'e0\u1083\'eb\u1080\'e8 \u1086\'ee\u1090\'f2 \u1089\'f1\u1085\'ed\u1077\'e5\u1075\'e3\u1086\'ee\u1074\'e2\u1099\'fb\u1093\'f5 \u1075\'e3\u1086\'ee\u1088\'f0, \u1086\'ee\u1073\'e1\u1077\'e5\u1097\'f9\u1072\'e0\u1103\'ff \u1075\'e3\u1088\'f0\u1086\'ee\u1079\'e7\u1091\'f3; \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1074\'e2\u1072\'e0 \u1052\'cc\u1072\'e0\u1096\'f8\u1091\'f3\u1082\'ea\u1072\'e0 \u1076\'e4\u1099\'fb\u1084\'ec\u1080\'e8\u1083\'eb\u1072\'e0\u1089\'f1\u1100\'fc, \u1082\'ea\u1072\'e0\u1082\'ea \u1079\'e7\u1072\'e0\u1075\'e3\u1072\'e0\u1096\'f8\u1077\'e5\u1085\'ed\u1085\'ed\u1099\'fb\u1081\'e9 \u1092\'f4\u1072\'e0\u1082\'ea\u1077\'e5\u1083\'eb; \u1082\'ea\u1088\'f0\u1091\'f3\u1075\'e3\u1086\'ee\u1084\'ec \u1085\'ed\u1077\'e5\u1075\'e3\u1086\'ee \u1074\'e2\u1080\'e8\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc \u1080\'e8 \u1087\'ef\u1086\'ee\u1083\'eb\u1079\'e7\u1072\'e0\u1083\'eb\u1080\'e8, \u1082\'ea\u1072\'e0\u1082\'ea \u1079\'e7\u1084\'ec\u1077\'e5\u1080\'e8, \u1089\'f1\u1077\'e5\u1088\'f0\u1099\'fb\u1077\'e5 \u1082\'ea\u1083\'eb\u1086\'ee\u1095\'f7\u1082\'ea\u1080\'e8 \u1086\'ee\u1073\'e1\u1083\'eb\u1072\'e0\u1082\'ea\u1086\'ee\u1074\'e2, \u1079\'e7\u1072\'e0\u1076\'e4\u1077\'e5\u1088\'f0\u1078\'e6\u1072\'e0\u1085\'ed\u1085\'ed\u1099\'fb\u1077\'e5 \u1074\'e2 \u1089\'f1\u1074\'e2\u1086\'ee\u1077\'e5\u1084\'ec \u1089\'f1\u1090\'f2\u1088\'f0\u1077\'e5\u1084\'ec\u1083\'eb\u1077\'e5\u1085\'ed\u1080\'e8\u1080\'e8 \u1080\'e8 \u1073\'e1\u1091\'f3\u1076\'e4\u1090\'f2\u1086\'ee \u1079\'e7\u1072\'e0\u1094\'f6\u1077\'e5\u1087\'ef\u1080\'e8\u1074\'e2\u1096\'f8\u1080\'e8\u1077\'e5\u1089\'f1\u1103\'ff \u1079\'e7\u1072\'e0 \u1082\'ea\u1086\'ee\u1083\'eb\u1102\'fe\u1095\'f7\u1080\'e8\u1081\'e9 \u1077\'e5\u1075\'e3\u1086\'ee \u1082\'ea\u1091\'f3\u1089\'f1\u1090\'f2\u1072\'e0\u1088\'f0\u1085\'ed\u1080\'e8\u1082\'ea. \u1042\'c2\u1086\'ee\u1079\'e7\u1076\'e4\u1091\'f3\u1093\'f5 \u1073\'e1\u1099\'fb\u1083\'eb \u1085\'ed\u1072\'e0\u1087\'ef\u1086\'ee\u1077\'e5\u1085\'ed \u1101\'fd\u1083\'eb\u1077\'e5\u1082\'ea\u1090\'f2\u1088\'f0\u1080\'e8\u1095\'f7\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee\u1084\'ec. \u1071\'df \u1091\'f3\u1075\'e3\u1083\'eb\u1091\'f3\u1073\'e1\u1080\'e8\u1083\'eb\u1089\'f1\u1103\'ff \u1074\'e2 \u1074\'e2\u1080\'e8\u1085\'ed\u1086\'ee\u1075\'e3\u1088\'f0\u1072\'e0\u1076\'e4\u1085\'ed\u1091\'f3\u1102\'fe \u1072\'e0\u1083\'eb\u1083\'eb\u1077\'e5\u1102\'fe, \u1074\'e2\u1077\'e5\u1076\'e4\u1091\'f3\u1097\'f9\u1091\'f3\u1102\'fe \u1074\'e2 \u1075\'e3\u1088\'f0\u1086\'ee\u1090\'f2; \u1084\'ec\u1085\'ed\u1077\'e5 \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee \u1075\'e3\u1088\'f0\u1091\'f3\u1089\'f1\u1090\'f2\u1085\'ed\u1086\'ee. \u1071\'df \u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1083\'eb \u1086\'ee \u1090\'f2\u1086\'ee\u1081\'e9 \u1084\'ec\u1086\'ee\u1083\'eb\u1086\'ee\u1076\'e4\u1086\'ee\u1081\'e9 \u1078\'e6\u1077\'e5\u1085\'ed\u1097\'f9\u1080\'e8\u1085\'ed\u1077\'e5 \u1089\'f1 \u1088\'f0\u1086\'ee\u1076\'e4\u1080\'e8\u1085\'ed\u1082\'ea\u1086\'ee\u1081\'e9 \u1085\'ed\u1072\'e0 \u1097\'f9\u1077\'e5\u1082\'ea\u1077\'e5, \u1087\'ef\u1088\'f0\u1086\'ee \u1082\'ea\u1086\'ee\u1090\'f2\u1086\'ee\u1088\'f0\u1091\'f3\u1102\'fe \u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1083\'eb \u1084\'ec\u1085\'ed\u1077\'e5 \u1076\'e4\u1086\'ee\u1082\'ea\u1090\'f2\u1086\'ee\u1088\'f0\u8230\'85 \u1047\'c7\u1072\'e0\u1095\'f7\u1077\'e5\u1084\'ec \u1086\'ee\u1085\'ed\u1072\'e0 \u1079\'e7\u1076\'e4\u1077\'e5\u1089\'f1\u1100\'fc? \u1048\'c8 \u1086\'ee\u1085\'ed\u1072\'e0 \u1083\'eb\u1080\'e8? \u1048\'c8 \u1087\'ef\u1086\'ee\u1095\'f7\u1077\'e5\u1084\'ec\u1091\'f3 \u1103\'ff \u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1102\'fe, \u1095\'f7\u1090\'f2\u1086\'ee \u1101\'fd\u1090\'f2\u1086\'ee \u1086\'ee\u1085\'ed\u1072\'e0? \u1080\'e8 \u1087\'ef\u1086\'ee\u1095\'f7\u1077\'e5\u1084\'ec\u1091\'f3 \u1103\'ff \u1076\'e4\u1072\'e0\u1078\'e6\u1077\'e5 \u1090\'f2\u1072\'e0\u1082\'ea \u1074\'e2 \u1101\'fd\u1090\'f2\u1086\'ee\u1084\'ec \u1091\'f3\u1074\'e2\u1077\'e5\u1088\'f0\u1077\'e5\u1085\'ed? \u1052\'cc\u1072\'e0\u1083\'eb\u1086\'ee \u1083\'eb\u1080\'e8 \u1078\'e6\u1077\'e5\u1085\'ed\u1097\'f9\u1080\'e8\u1085\'ed \u1089\'f1 \u1088\'f0\u1086\'ee\u1076\'e4\u1080\'e8\u1085\'ed\u1082\'ea\u1072\'e0\u1084\'ec\u1080\'e8 \u1085\'ed\u1072\'e0 \u1097\'f9\u1077\'e5\u1082\'ea\u1072\'e0\u1093\'f5? \u1056\'d0\u1072\'e0\u1079\'e7\u1084\'ec\u1099\'fb\u1096\'f8\u1083\'eb\u1103\'ff\u1103\'ff \u1090\'f2\u1072\'e0\u1082\'ea\u1080\'e8\u1084\'ec \u1086\'ee\u1073\'e1\u1088\'f0\u1072\'e0\u1079\'e7\u1086\'ee\u1084\'ec, \u1103\'ff \u1087\'ef\u1086\'ee\u1076\'e4\u1086\'ee\u1096\'f8\u1077\'e5\u1083\'eb \u1082\'ea \u1089\'f1\u1072\'e0\u1084\'ec\u1086\'ee\u1084\'ec\u1091\'f3 \u1075\'e3\u1088\'f0\u1086\'ee\u1090\'f2\u1091\'f3. \u1057\'d1\u1084\'ec\u1086\'ee\u1090\'f2\u1088\'f0\u1102\'fe: \u1074\'e2 \u1087\'ef\u1088\'f0\u1086\'ee\u1093\'f5\u1083\'eb\u1072\'e0\u1076\'e4\u1085\'ed\u1086\'ee\u1081\'e9 \u1090\'f2\u1077\'e5\u1085\'ed\u1080\'e8 \u1077\'e5\u1075\'e3\u1086\'ee \u1089\'f1\u1074\'e2\u1086\'ee\u1076\'e4\u1072\'e0, \u1085\'ed\u1072\'e0 \u1082\'ea\u1072\'e0\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1081\'e9 \u1089\'f1\u1082\'ea\u1072\'e0\u1084\'ec\u1100\'fc\u1077\'e5 \u1089\'f1\u1080\'e8\u1076\'e4\u1080\'e8\u1090\'f2 \u1078\'e6\u1077\'e5\u1085\'ed\u1097\'f9\u1080\'e8\u1085\'ed\u1072\'e0, \u1074\'e2 \u1089\'f1\u1086\'ee\u1083\'eb\u1086\'ee\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1081\'e9 \u1096\'f8\u1083\'eb\u1103\'ff\u1087\'ef\u1082\'ea\u1077\'e5, \u1086\'ee\u1082\'ea\u1091\'f3\u1090\'f2\u1072\'e0\u1085\'ed\u1085\'ed\u1072\'e0\u1103\'ff \u1095\'f7\u1077\'e5\u1088\'f0\u1085\'ed\u1086\'ee\u1081\'e9 \u1096\'f8\u1072\'e0\u1083\'eb\u1100\'fc\u1102\'fe, \u1086\'ee\u1087\'ef\u1091\'f3\u1089\'f1\u1090\'f2\u1080\'e8\u1074\'e2 \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1074\'e2\u1091\'f3 \u1085\'ed\u1072\'e0 \u1075\'e3\u1088\'f0\u1091\'f3\u1076\'e4\u1100\'fc; \u1096\'f8\u1083\'eb\u1103\'ff\u1087\'ef\u1082\'ea\u1072\'e0 \u1079\'e7\u1072\'e0\u1082\'ea\u1088\'f0\u1099\'fb\u1074\'e2\u1072\'e0\u1083\'eb\u1072\'e0 \u1077\'e5\u1077\'e5 \u1083\'eb\u1080\'e8\u1094\'f6\u1086\'ee. \u1071\'df \u1093\'f5\u1086\'ee\u1090\'f2\u1077\'e5\u1083\'eb \u1091\'f3\u1078\'e6\u1077\'e5 \u1074\'e2\u1077\'e5\u1088\'f0\u1085\'ed\u1091\'f3\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1 \u1085\'ed\u1077\'e5 \u1085\'ed\u1072\'e0\u1088\'f0\u1091\'f3\u1096\'f8\u1080\'e8\u1090\'f2\u1100\'fc \u1077\'e5\u1077\'e5 \u1084\'ec\u1077\'e5\u1095\'f7\u1090\'f2\u1072\'e0\u1085\'ed\u1080\'e8\u1081\'e9, \u1082\'ea\u1086\'ee\u1075\'e3\u1076\'e4\u1072\'e0 \u1086\'ee\u1085\'ed\u1072\'e0 \u1085\'ed\u1072\'e0 \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff \u1074\'e2\u1079\'e7\u1075\'e3\u1083\'eb\u1103\'ff\u1085\'ed\u1091\'f3\u1083\'eb\u1072\'e0.}
